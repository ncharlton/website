{% extends 'admin/admin_base.html.twig' %}

{% block pageheader %}New news{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-lg-12">
            <div class="block">
                <div class="title"><strong class="d-block">New news</strong></div>
                <div class="block-body">
                    {% form_theme form 'admin/form/jquery.collection.html.twig' %}

                    {{ form_start(form) }}
                    {{ form_widget(form) }}
                    <button class="btn btn-primary" type="submit">Create</button>
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </div>

    <script type="text/javascript">
        $('.tags').collection();
    </script>
{% endblock %}


